		<trigger>
			<name>TG_MES_MTRL_STOCK</name>
			<schema>KUMMUN</schema>
			<sql>CREATE OR REPLACE TRIGGER KUMMUN.&quot;TG_TB_MES_MTRL_STOCK&quot;&#x0D;&#x0A;AFTER INSERT OR UPDATE ON KUMMUN.TB_MES_MTRL_RSLT&#x0D;&#x0A;REFERENCING NEW AS NEW OLD AS OLD&#x0D;&#x0A;&#x09;&#x0D;&#x0A;&#x09; FOR EACH ROW&#x0D;&#x0A;&#x09;&#x0D;&#x0A;&#x09;DECLARE&#x0D;&#x0A;&#x09;&#x0D;&#x0A;&#x09;V_CODE NUMBER;&#x0D;&#x0A;&#x09;V_ERRM VARCHAR2(64);&#x0D;&#x0A;&#x09;&#x0D;&#x0A;&#x09; BEGIN &#x0D;&#x0A;&#x09;--2차계량 등록되어있으며 소재이력테이블의 프로시저를 통해 값이 생성된 경우 소재테이블에 로스율반영된중량을 차감한다.&#x0D;&#x0A;&#x09; IF (:NEW.CREATED_PROGRAM_ID = &apos;SP_MTRL_STOCK_SUBTRACT&apos; AND :NEW.MTRL_CHG_CAUSE_CD = &apos;8&apos; AND :NEW.ALLOC_VEHL_NO IS NOT NULL)&#x0D;&#x0A;&#x09; THEN&#x0D;&#x0A;&#x09; &#x0D;&#x0A;&#x09; MERGE INTO KUMMUN.TB_MES_MTRL_STOCK &#x0D;&#x0A;&#x09; USING DUAL&#x0D;&#x0A;&#x09; ON ( WORKS_CD = :NEW.WORKS_CD&#x0D;&#x0A;&#x09;  &#x09;&#x09;AND FAC_CD = :NEW.FAC_CD&#x0D;&#x0A;&#x09;  &#x09;&#x09;AND SUP_CUST_CD = :NEW.SUP_CUST_CD&#x0D;&#x0A;&#x09;  &#x09;&#x09;AND PRODUCT_NAME_CD = :NEW.PRODUCT_NAME_CD&#x0D;&#x0A;&#x09;  &#x09;&#x09;AND DIMS_CD = :NEW.DIMS_CD&#x0D;&#x0A;&#x09;  &#x09;&#x09;AND CUST_CD = :NEW.CUST_CD )&#x0D;&#x0A;&#x09;WHEN MATCHED THEN&#x0D;&#x0A;&#x09;&#x09;UPDATE SET &#x0D;&#x0A;&#x09;&#x09;MTRL_STOCK_WGT  = NVL(MTRL_STOCK_WGT, 0) + :NEW.LOSS_MTRL_WGT&#x0D;&#x0A;&#x09;&#x09;, LAST_UPDATED_OBJECT_ID = &apos;TG&apos;&#x0D;&#x0A;&#x09;&#x09;, LAST_UPDATE_PROGRAM_ID = &apos;TG_TB_MES_MTRL_STOCK_1&apos;&#x0D;&#x0A;&#x09;&#x09;, LAST_UPDATE_TIMESTAMP = SYSDATE&#x0D;&#x0A;&#x09;WHEN NOT MATCHED THEN   &#x0D;&#x0A;&#x09;INSERT &#x0D;&#x0A;&#x09; ( WORKS_CD,&#x0D;&#x0A;&#x09;   FAC_CD,&#x0D;&#x0A;&#x09;   SUP_CUST_CD,&#x0D;&#x0A;&#x09;   CUST_CD,&#x0D;&#x0A;&#x09;   PRODUCT_NAME_CD,&#x0D;&#x0A;&#x09;   DIMS_CD,&#x0D;&#x0A;&#x09;   SPEC_CD,&#x0D;&#x0A;&#x09;   MTRL_STOCK_WGT,&#x0D;&#x0A;&#x09;   CREATED_OBJECT_ID,&#x0D;&#x0A;&#x09;   CREATED_PROGRAM_ID,&#x0D;&#x0A;&#x09;   CREATION_TIMESTAMP,&#x0D;&#x0A;&#x09;   LAST_UPDATED_OBJECT_ID,&#x0D;&#x0A;&#x09;   LAST_UPDATE_PROGRAM_ID,&#x0D;&#x0A;&#x09;   LAST_UPDATE_TIMESTAMP )&#x0D;&#x0A;&#x09; VALUES &#x0D;&#x0A;&#x09; ( :NEW.WORKS_CD,&#x0D;&#x0A;&#x09;   :NEW.FAC_CD,&#x0D;&#x0A;&#x09;   :NEW.SUP_CUST_CD,&#x0D;&#x0A;&#x09;   :NEW.CUST_CD,&#x0D;&#x0A;&#x09;   :NEW.PRODUCT_NAME_CD,&#x0D;&#x0A;&#x09;   :NEW.DIMS_CD,&#x0D;&#x0A;&#x09;   NVL(:NEW.SPEC_CD, &apos; &apos;),&#x0D;&#x0A;&#x09;   NVL(:NEW.LOSS_MTRL_WGT,0),&#x0D;&#x0A;&#x09;   &apos;JA&apos;,&#x0D;&#x0A;&#x09;   &apos;TG_TB_MES_MTRL_STOCK_1&apos;,&#x0D;&#x0A;&#x09;   SYSDATE,&#x0D;&#x0A;&#x09;   &apos;JA&apos;,&#x0D;&#x0A;&#x09;   &apos;TG_TB_MES_MTRL_STOCK_1&apos;,&#x0D;&#x0A;&#x09;   SYSDATE&#x0D;&#x0A;&#x09;    );&#x0D;&#x0A;&#x09;END IF;&#x0D;&#x0A;&#x09;--TB_CUST_INFO_DTL에서 LOSS_RATE가 변경 된 경우(=TG_UPDATE_LOSS_RATE 트리거를 통해 값이 생성된 경우) 소재테이블에 로스율반영된중량을 차감한다.&#x0D;&#x0A;&#x09;IF ( :NEW.CREATED_PROGRAM_ID = &apos;TG_UPDATE_LOSS_RATE&apos; AND :NEW.MTRL_CHG_CAUSE_CD = &apos;10&apos;)&#x0D;&#x0A;&#x09;  &#x0D;&#x0A;&#x09; THEN&#x0D;&#x0A;&#x09; &#x0D;&#x0A;&#x09; MERGE INTO KUMMUN.TB_MES_MTRL_STOCK &#x0D;&#x0A;&#x09; USING DUAL&#x0D;&#x0A;&#x09; ON ( WORKS_CD = :NEW.WORKS_CD&#x0D;&#x0A;&#x09;  &#x09;&#x09;AND FAC_CD = :NEW.FAC_CD&#x0D;&#x0A;&#x09;  &#x09;&#x09;AND SUP_CUST_CD = :NEW.SUP_CUST_CD&#x0D;&#x0A;&#x09;  &#x09;&#x09;AND PRODUCT_NAME_CD = :NEW.PRODUCT_NAME_CD&#x0D;&#x0A;&#x09;  &#x09;&#x09;AND DIMS_CD = :NEW.DIMS_CD&#x0D;&#x0A;&#x09;  &#x09;&#x09;AND CUST_CD = :NEW.CUST_CD )&#x0D;&#x0A;&#x09;WHEN MATCHED THEN&#x0D;&#x0A;&#x09;&#x09;UPDATE SET &#x0D;&#x0A;&#x09;&#x09;MTRL_STOCK_WGT  =  MTRL_STOCK_WGT + :NEW.LOSS_MTRL_WGT&#x0D;&#x0A;&#x09;&#x09;, LAST_UPDATED_OBJECT_ID = &apos;TG&apos;&#x0D;&#x0A;&#x09;&#x09;, LAST_UPDATE_PROGRAM_ID = &apos;TG_TB_MES_MTRL_STOCK_2&apos;&#x0D;&#x0A;&#x09;&#x09;, LAST_UPDATE_TIMESTAMP = SYSDATE&#x0D;&#x0A;&#x09;WHEN NOT MATCHED THEN   &#x0D;&#x0A;&#x09;INSERT &#x0D;&#x0A;&#x09; ( WORKS_CD,&#x0D;&#x0A;&#x09;   FAC_CD,&#x0D;&#x0A;&#x09;   SUP_CUST_CD,&#x0D;&#x0A;&#x09;   CUST_CD,&#x0D;&#x0A;&#x09;   PRODUCT_NAME_CD,&#x0D;&#x0A;&#x09;   DIMS_CD,&#x0D;&#x0A;&#x09;   SPEC_CD,&#x0D;&#x0A;&#x09;   MTRL_STOCK_WGT,&#x0D;&#x0A;&#x09;   CREATED_OBJECT_ID,&#x0D;&#x0A;&#x09;   CREATED_PROGRAM_ID,&#x0D;&#x0A;&#x09;   CREATION_TIMESTAMP,&#x0D;&#x0A;&#x09;   LAST_UPDATED_OBJECT_ID,&#x0D;&#x0A;&#x09;   LAST_UPDATE_PROGRAM_ID,&#x0D;&#x0A;&#x09;   LAST_UPDATE_TIMESTAMP )&#x0D;&#x0A;&#x09; VALUES &#x0D;&#x0A;&#x09; ( :NEW.WORKS_CD,&#x0D;&#x0A;&#x09;   :NEW.FAC_CD,&#x0D;&#x0A;&#x09;   :NEW.SUP_CUST_CD,&#x0D;&#x0A;&#x09;   :NEW.CUST_CD,&#x0D;&#x0A;&#x09;   :NEW.PRODUCT_NAME_CD,&#x0D;&#x0A;&#x09;   :NEW.DIMS_CD,&#x0D;&#x0A;&#x09;   NVL(:NEW.SPEC_CD, &apos; &apos;),&#x0D;&#x0A;&#x09;   :NEW.MTRL_WGT + :NEW.LOSS_MTRL_WGT,&#x0D;&#x0A;&#x09;   &apos;JA&apos;,&#x0D;&#x0A;&#x09;   &apos;TG_TB_MES_MTRL_STOCK_2&apos;,&#x0D;&#x0A;&#x09;   SYSDATE,&#x0D;&#x0A;&#x09;   &apos;JA&apos;,&#x0D;&#x0A;&#x09;   &apos;TG_TB_MES_MTRL_STOCK_2&apos;,&#x0D;&#x0A;&#x09;   SYSDATE&#x0D;&#x0A;&#x09;    );&#x0D;&#x0A;&#x09;END IF;&#x0D;&#x0A;&#x09;&#x0D;&#x0A;&#x09;--프로시저 SP_IF_WEIGHTING_RECV_J_IN 를 통해 MTRL_CHG_CAUSE_CD값이 &apos;3&apos;-&gt; &apos;1&apos;로 변경된 경우&#x0D;&#x0A;&#x09; IF (:NEW.LAST_UPDATE_PROGRAM_ID = &apos;SP_IF_WEIGHTING_RECV_J_IN&apos; AND :NEW.MTRL_CHG_CAUSE_CD = &apos;1&apos; AND :NEW.WGHT_NO IS NOT NULL)&#x0D;&#x0A;&#x09; THEN&#x0D;&#x0A;&#x09; &#x0D;&#x0A;&#x09; MERGE INTO KUMMUN.TB_MES_MTRL_STOCK &#x0D;&#x0A;&#x09; USING DUAL&#x0D;&#x0A;&#x09; ON ( WORKS_CD = :NEW.WORKS_CD&#x0D;&#x0A;&#x09;  &#x09;&#x09;AND FAC_CD = :NEW.FAC_CD&#x0D;&#x0A;&#x09;  &#x09;&#x09;AND SUP_CUST_CD = :NEW.SUP_CUST_CD&#x0D;&#x0A;&#x09;  &#x09;&#x09;AND PRODUCT_NAME_CD = :NEW.PRODUCT_NAME_CD&#x0D;&#x0A;&#x09;  &#x09;&#x09;AND DIMS_CD = :NEW.DIMS_CD&#x0D;&#x0A;&#x09;  &#x09;&#x09;AND CUST_CD = :NEW.CUST_CD )&#x0D;&#x0A;&#x09;WHEN MATCHED THEN&#x0D;&#x0A;&#x09;&#x09;UPDATE SET &#x0D;&#x0A;&#x09;&#x09;MTRL_STOCK_WGT  = NVL(MTRL_STOCK_WGT, 0) + NVL(:NEW.MTRL_WGT, 0 )&#x0D;&#x0A;&#x09;&#x09;, LAST_UPDATED_OBJECT_ID = &apos;TG&apos;&#x0D;&#x0A;&#x09;&#x09;, LAST_UPDATE_PROGRAM_ID = &apos;TG_TB_MES_MTRL_STOCK_3&apos;&#x0D;&#x0A;&#x09;&#x09;, LAST_UPDATE_TIMESTAMP = SYSDATE&#x0D;&#x0A;&#x09;WHEN NOT MATCHED THEN   &#x0D;&#x0A;&#x09;INSERT &#x0D;&#x0A;&#x09; ( WORKS_CD,&#x0D;&#x0A;&#x09;   FAC_CD,&#x0D;&#x0A;&#x09;   SUP_CUST_CD,&#x0D;&#x0A;&#x09;   CUST_CD,&#x0D;&#x0A;&#x09;   PRODUCT_NAME_CD,&#x0D;&#x0A;&#x09;   DIMS_CD,&#x0D;&#x0A;&#x09;   SPEC_CD,&#x0D;&#x0A;&#x09;   MTRL_STOCK_WGT,&#x0D;&#x0A;&#x09;   CREATED_OBJECT_ID,&#x0D;&#x0A;&#x09;   CREATED_PROGRAM_ID,&#x0D;&#x0A;&#x09;   CREATION_TIMESTAMP,&#x0D;&#x0A;&#x09;   LAST_UPDATED_OBJECT_ID,&#x0D;&#x0A;&#x09;   LAST_UPDATE_PROGRAM_ID,&#x0D;&#x0A;&#x09;   LAST_UPDATE_TIMESTAMP )&#x0D;&#x0A;&#x09; VALUES &#x0D;&#x0A;&#x09; ( :NEW.WORKS_CD,&#x0D;&#x0A;&#x09;   :NEW.FAC_CD,&#x0D;&#x0A;&#x09;   :NEW.SUP_CUST_CD,&#x0D;&#x0A;&#x09;   :NEW.CUST_CD,&#x0D;&#x0A;&#x09;   :NEW.PRODUCT_NAME_CD,&#x0D;&#x0A;&#x09;   :NEW.DIMS_CD,&#x0D;&#x0A;&#x09;   NVL(:NEW.SPEC_CD, &apos; &apos;),&#x0D;&#x0A;&#x09;   NVL(:NEW.MTRL_WGT,0),&#x0D;&#x0A;&#x09;   &apos;TG&apos;,&#x0D;&#x0A;&#x09;   &apos;TG_TB_MES_MTRL_STOCK_3&apos;,&#x0D;&#x0A;&#x09;   SYSDATE,&#x0D;&#x0A;&#x09;   &apos;TG&apos;,&#x0D;&#x0A;&#x09;   &apos;TG_TB_MES_MTRL_STOCK_3&apos;,&#x0D;&#x0A;&#x09;   SYSDATE&#x0D;&#x0A;&#x09;    );&#x0D;&#x0A;&#x09;END IF;&#x0D;&#x0A;&#x09;EXCEPTION&#x0D;&#x0A;&#x09;  WHEN NO_DATA_FOUND THEN&#x0D;&#x0A;    &#x09; DBMS_OUTPUT.PUT_LINE(&apos;DATA NOT FOUND!, TG_TB_MES_MTRL_STOCK&apos;);&#x0D;&#x0A;&#x09;  WHEN OTHERS THEN&#x0D;&#x0A;&#x09;  &#x09;V_CODE := SQLCODE;&#x0D;&#x0A;&#x09;  &#x09;V_ERRM := SUBSTR(SQLERRM, 1, 64);&#x0D;&#x0A;&#x09;  &#x09;DBMS_OUTPUT.PUT_LINE(&apos;TG_TB_MES_MTRL_STOCK: THE ERROR CODE IS &apos; || V_CODE || &apos;-&apos; || V_ERRM);&#x0D;&#x0A;&#x09;&#x0D;&#x0A;&#x09;&#x0D;&#x0A;END;</sql>
			<description></description>
		</trigger>
		<trigger>
			<name>TG_UPDATE_LOSS_RATE</name>
			<schema>KUMMUN</schema>
			<sql>CREATE OR REPLACE TRIGGER KUMMUN.&quot;TG_UPDATE_LOSS_RATE&quot;&#x0D;&#x0A;AFTER INSERT OR UPDATE ON KUMMUN.TB_MES_CUST_INFO_DTL&#x0D;&#x0A;REFERENCING NEW AS NEW OLD AS OLD&#x0D;&#x0A;&#x0D;&#x0A;FOR EACH ROW &#x0D;&#x0A;&#x0D;&#x0A;DECLARE&#x0D;&#x0A;  V_COUNT NUMBER(10) DEFAULT 0;&#x0D;&#x0A;  V_SUP_CUST_CD VARCHAR2(50);&#x0D;&#x0A;  V_CODE NUMBER;&#x0D;&#x0A;  V_ERRM VARCHAR2(64);&#x0D;&#x0A;  &#x0D;&#x0A;BEGIN &#x0D;&#x0A;&#x0D;&#x0A;  SELECT ORD_COMP_CD &#x0D;&#x0A;  INTO V_SUP_CUST_CD&#x0D;&#x0A;  FROM KUMMUN.TB_MES_CUST_INFO &#x0D;&#x0A;  WHERE WORKS_CD = :NEW.WORKS_CD AND FAC_CD = :NEW.FAC_CD AND CUST_TP = &apos;1&apos;&#x0D;&#x0A;    AND CUST_CD = :NEW.CUST_CD ;&#x0D;&#x0A;  &#x0D;&#x0A;  SELECT COUNT(*) INTO V_COUNT &#x0D;&#x0A;  FROM KUMMUN.TB_MES_MTRL_RSLT&#x0D;&#x0A;  WHERE  WORKS_CD = :NEW.WORKS_CD AND FAC_CD = :NEW.FAC_CD&#x0D;&#x0A;      AND CUST_CD = :NEW.CUST_CD &#x0D;&#x0A;      AND SUP_CUST_CD = V_SUP_CUST_CD &#x0D;&#x0A;      AND DIMS_CD = :NEW.DIMS_CD &#x0D;&#x0A;      AND MTRL_CHG_CAUSE_CD = &apos;8&apos;;&#x0D;&#x0A;&#x0D;&#x0A;  &#x0D;&#x0A;  IF ( V_COUNT &gt; 0 ) THEN &#x0D;&#x0A;    &#x0D;&#x0A;   DBMS_OUTPUT.PUT_LINE(&apos; V_COUNT &gt; 0 &apos; || V_COUNT );&#x0D;&#x0A;   INSERT INTO KUMMUN.TB_MES_MTRL_RSLT &#x0D;&#x0A;    (TRANSACTION_ID&#x0D;&#x0A;    , WORKS_CD&#x0D;&#x0A;    , FAC_CD&#x0D;&#x0A;    , LOSS_RATE&#x0D;&#x0A;    , LOSS_MTRL_WGT &#x0D;&#x0A;    , BASE_DD&#x0D;&#x0A;    , MTRL_CHG_CAUSE_CD&#x0D;&#x0A;    , SUP_CUST_CD&#x0D;&#x0A;    , CUST_CD&#x0D;&#x0A;    , PRODUCT_NAME_CD&#x0D;&#x0A;    , DIMS_CD&#x0D;&#x0A;    , MTRL_WGT&#x0D;&#x0A;    , MTRL_CHG_CAUSE&#x0D;&#x0A;    , CREATED_OBJECT_ID&#x0D;&#x0A;    , CREATED_PROGRAM_ID&#x0D;&#x0A;    , CREATION_TIMESTAMP&#x0D;&#x0A;    , LAST_UPDATED_OBJECT_ID&#x0D;&#x0A;    , LAST_UPDATE_PROGRAM_ID&#x0D;&#x0A;    , LAST_UPDATE_TIMESTAMP) &#x0D;&#x0A;  SELECT&#x0D;&#x0A;    SQ_MES_MTRL_RSLT.nextval&#x0D;&#x0A;    , :NEW.WORKS_CD AS WORKS_CD&#x0D;&#x0A;    , :NEW.FAC_CD AS FAC_CD&#x0D;&#x0A;    , :NEW.LOSS_RATE AS LOSS_RATE&#x0D;&#x0A;    , A.LOSS_MTRL_WGT&#x0D;&#x0A;    , TO_CHAR(SYSDATE, &apos;YYYYMMDD&apos;) AS BASE_DD&#x0D;&#x0A;    , &apos;10&apos; AS MTRL_CHG_CAUSE_CD&#x0D;&#x0A;    , A.SUP_CUST_CD&#x0D;&#x0A;    , A.CUST_CD&#x0D;&#x0A;    , A.PRODUCT_NAME_CD&#x0D;&#x0A;    , A.DIMS_CD&#x0D;&#x0A;    , A.MTRL_WGT&#x0D;&#x0A;    , &apos;LOSS율변경&apos; AS MTRL_CHG_CAUSE&#x0D;&#x0A;    , &apos;JA&apos; AS CREATED_OBJECT_ID&#x0D;&#x0A;    , &apos;TG_UPDATE_LOSS_RATE&apos; AS CREATED_PROGRAM_ID&#x0D;&#x0A;    , SYSDATE AS CREATION_TIMESTAMP&#x0D;&#x0A;    , &apos;JA&apos; AS LAST_UPDATED_OBJECT_ID&#x0D;&#x0A;    , &apos;TG_UPDATE_LOSS_RATE&apos; AS LAST_UPDATE_PROGRAM_ID&#x0D;&#x0A;    , SYSDATE AS LAST_UPDATE_TIMESTAMP&#x0D;&#x0A;    FROM (&#x0D;&#x0A;      SELECT   &#x0D;&#x0A;                    A.SUP_CUST_CD&#x0D;&#x0A;            , A.CUST_CD&#x0D;&#x0A;            , A.DIMS_CD&#x0D;&#x0A;            , A.PRODUCT_NAME_CD&#x0D;&#x0A;            , ROUND (SUM(A.MTRL_WGT)* ((B.LOSS_RATE -:NEW.LOSS_RATE)/100)) AS LOSS_MTRL_WGT&#x0D;&#x0A;            , SUM(A.MTRL_WGT) AS MTRL_WGT&#x0D;&#x0A;        FROM KUMMUN.TB_MES_MTRL_RSLT A,&#x0D;&#x0A;        (SELECT  LOSS_RATE, WORKS_CD, FAC_CD, CUST_CD, SUP_CUST_CD, DIMS_CD&#x0D;&#x0A;              FROM KUMMUN.TB_MES_MTRL_RSLT WHERE  WORKS_CD = :NEW.WORKS_CD&#x0D;&#x0A;              AND FAC_CD = :NEW.FAC_CD &#x0D;&#x0A;              AND CUST_CD = :NEW.CUST_CD&#x0D;&#x0A;              AND SUP_CUST_CD = V_SUP_CUST_CD&#x0D;&#x0A;              AND DIMS_CD = :NEW.DIMS_CD&#x0D;&#x0A;              AND MTRL_CHG_CAUSE_CD = &apos;8&apos; &#x0D;&#x0A;              AND ROWNUM = 1&#x0D;&#x0A;              ORDER BY LAST_UPDATE_TIMESTAMP DESC) B&#x0D;&#x0A;        WHERE A.WORKS_CD = B.WORKS_CD &#x0D;&#x0A;        AND A.FAC_CD = B.FAC_CD &#x0D;&#x0A;        AND A.CUST_CD = B.CUST_CD &#x0D;&#x0A;        AND A.SUP_CUST_CD = B.SUP_CUST_CD &#x0D;&#x0A;        AND A.DIMS_CD = B.DIMS_CD &#x0D;&#x0A;        AND A.WORKS_CD = :NEW.WORKS_CD&#x0D;&#x0A;              AND A.FAC_CD = :NEW.FAC_CD&#x0D;&#x0A;              AND A.CUST_CD =:NEW.CUST_CD&#x0D;&#x0A;              AND A.SUP_CUST_CD = V_SUP_CUST_CD&#x0D;&#x0A;              AND A.DIMS_CD = :NEW.DIMS_CD&#x0D;&#x0A;              AND A.MTRL_CHG_CAUSE_CD = &apos;8&apos;&#x0D;&#x0A;              AND A.LAST_UPDATE_TIMESTAMP &lt; SYSDATE&#x0D;&#x0A;        GROUP BY  &#x0D;&#x0A;         A.SUP_CUST_CD, A.CUST_CD, A.PRODUCT_NAME_CD, A.DIMS_CD, B.LOSS_RATE )A ;&#x0D;&#x0A;     &#x0D;&#x0A;  END IF;&#x0D;&#x0A;&#x0D;&#x0A; EXCEPTION&#x0D;&#x0A;    WHEN NO_DATA_FOUND THEN&#x0D;&#x0A;       DBMS_OUTPUT.PUT_LINE(&apos;DATA NOT FOUND!, TG_UPDATE_LOSS_RATE&apos;);&#x0D;&#x0A;    WHEN OTHERS THEN&#x0D;&#x0A;      V_CODE := SQLCODE;&#x0D;&#x0A;      V_ERRM := SUBSTR(SQLERRM, 1, 64);&#x0D;&#x0A;      DBMS_OUTPUT.PUT_LINE(&apos;TG_UPDATE_LOSS_RATE: THE ERROR CODE IS &apos; || V_CODE || &apos;-&apos; || V_ERRM);&#x0D;&#x0A;  &#x0D;&#x0A;END;</sql>
			<description>거래처의 직경, 강종, 계약 로스율이 바뀐다면 해당 거래처에 대한 공급업체의 소재량이 로스율에 따라 차감된다. 재고변동이력 테이블의 재고변동사유 (MTRL_CHG_CAUSE_CD): 8(출하완료)인 건들을 변경된 로스율을 계산해 소재량을 반영한다. 이때 (MTRL_CHG_CAUSE_CD 코드는 10(LOSS율변경) 으로 INSERT</description>
		</trigger>
	</trigger_set>
