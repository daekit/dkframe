			<text>/*진천 계량 프로시저 &#x0D;&#x0A;&#x0D;&#x0A;&#x0D;&#x0A;&#x0D;&#x0A;*/&#x0D;&#x0A;CREATE OR REPLACE PROCEDURE KUMMUN.&quot;SP_IF_MTRL_STOCK_RECV_J&quot;   &#x0D;&#x0A;IS&#x0D;&#x0A;  CURSOR CUR_1 IS&#x0D;&#x0A;       SELECT *&#x0D;&#x0A;       FROM  TB_MES_IF_MTRL_STOCK&#x0D;&#x0A;       WHERE NVL(RECV_FLAG,&apos;N&apos;) = &apos;N&apos; &#x0D;&#x0A;        AND WORKS_CD = &apos;J&apos;&#x0D;&#x0A;       ORDER BY WGHT_NO DESC ;&#x0D;&#x0A;&#x0D;&#x0A;       -- 커서를 패치하기 위한 편수 선언&#x0D;&#x0A;    CUR1_R   TB_MES_IF_MTRL_STOCK%ROWTYPE;&#x0D;&#x0A;  &#x0D;&#x0A;  V_WORKS_CD    VARCHAR2(1)    DEFAULT &apos;J&apos;;&#x0D;&#x0A;  V_FAC_CD    VARCHAR2(4)    DEFAULT &apos;1146&apos;;&#x0D;&#x0A;  V_SUP_CUST_CD     VARCHAR2(10)  DEFAULT &apos;4001&apos; ;  --제강사 동국제강 CUST_CD: 4001 &#x0D;&#x0A;&#x0D;&#x0A;  V_FETCH_CNT   NUMBER(3)      DEFAULT 0;&#x0D;&#x0A;  V_CUST_CD       VARCHAR2(20)   DEFAULT &apos;3333&apos;; --제강사의 경우 현장명을 알 수 없으므로 DEFALUT값 지정&#x0D;&#x0A;  &#x0D;&#x0A;  V_MTRL_STOCK_CNT NUMBER(3)     DEFAULT 0;&#x0D;&#x0A;  V_PROD_DIMS      VARCHAR2(120) DEFAULT NULL;&#x0D;&#x0A;  V_DIMS_CD        VARCHAR2(20)  DEFAULT NULL;&#x0D;&#x0A;  V_PROC_SW        VARCHAR2(1)   DEFAULT &apos;N&apos;;&#x0D;&#x0A;  V_MTRL_CHG_CD  VARCHAR2(20)  DEFAULT NULL;&#x0D;&#x0A;  V_MTRL_CHG_CAUSE VARCHAR2(20) DEFAULT NULL;&#x0D;&#x0A;  &#x0D;&#x0A;BEGIN&#x0D;&#x0A;     &#x0D;&#x0A;   -- V_SUP_CUST_CD := NULL; &#x0D;&#x0A;  &#x0D;&#x0A;  DBMS_OUTPUT.PUT_LINE(&apos;V_SUP_CUST_CD : &apos; ||V_SUP_CUST_CD); &#x0D;&#x0A;      &#x0D;&#x0A;               &#x0D;&#x0A;    OPEN CUR_1;&#x0D;&#x0A;    LOOP&#x0D;&#x0A;     -- 커서의 패치 &#x0D;&#x0A;      FETCH CUR_1 INTO CUR1_R;&#x0D;&#x0A;      EXIT WHEN CUR_1%NOTFOUND;&#x0D;&#x0A;  &#x0D;&#x0A;      V_FETCH_CNT := V_FETCH_CNT + 1;&#x0D;&#x0A;      &#x0D;&#x0A;      DBMS_OUTPUT.PUT_LINE(&apos;V_FETCH_CNT : &apos; ||V_FETCH_CNT); &#x0D;&#x0A;      DBMS_OUTPUT.PUT_LINE(&apos;IF_XI_GROUP_ID : &apos; || CUR1_R.IF_XI_GROUP_ID); &#x0D;&#x0A;      &#x0D;&#x0A;      V_SUP_CUST_CD := &apos;4001&apos;;    --임시사용 --제강사:동국제강&#x0D;&#x0A;      &#x0D;&#x0A;  &#x0D;&#x0A;        IF  CUR1_R.PROD_DIMS IS NULL  THEN&#x0D;&#x0A;            DBMS_OUTPUT.PUT_LINE(&apos;PROD_DIMS IS NULL&apos;); &#x0D;&#x0A;             &#x0D;&#x0A;            UPDATE TB_MES_IF_MTRL_STOCK SET  &#x0D;&#x0A;                 ERR_MSG   =   &apos;PROD_DIMS IS NULL&apos;&#x0D;&#x0A;          WHERE  WORKS_CD       = CUR1_R.WORKS_CD&#x0D;&#x0A;          --  AND  FAC_CD         = CUR1_R.FAC_CD&#x0D;&#x0A;            AND  IF_XI_GROUP_ID = CUR1_R.IF_XI_GROUP_ID; &#x0D;&#x0A;             &#x0D;&#x0A;        CONTINUE;&#x0D;&#x0A;        END IF;&#x0D;&#x0A;        &#x0D;&#x0A;         IF  CUR1_R.SPEC_CD IS NULL  THEN&#x0D;&#x0A;            DBMS_OUTPUT.PUT_LINE(&apos;SPEC_CD IS NULL&apos;); &#x0D;&#x0A;             &#x0D;&#x0A;            UPDATE TB_MES_IF_MTRL_STOCK SET  &#x0D;&#x0A;                 ERR_MSG   =   &apos;SPEC_CD IS NULL&apos;&#x0D;&#x0A;          WHERE  WORKS_CD       = CUR1_R.WORKS_CD&#x0D;&#x0A;           -- AND  FAC_CD         = CUR1_R.FAC_CD&#x0D;&#x0A;            AND  IF_XI_GROUP_ID = CUR1_R.IF_XI_GROUP_ID; &#x0D;&#x0A;             &#x0D;&#x0A;        CONTINUE;&#x0D;&#x0A;        END IF;&#x0D;&#x0A;    &#x0D;&#x0A;    V_PROD_DIMS := CUR1_R.PROD_DIMS;&#x0D;&#x0A;    DBMS_OUTPUT.PUT_LINE(&apos;V_PROD_DIMS A:&apos; || V_PROD_DIMS);&#x0D;&#x0A;    DBMS_OUTPUT.PUT_LINE(&apos;CUR1_R.SPEC_CD:&apos; || CUR1_R.SPEC_CD);&#x0D;&#x0A;    &#x0D;&#x0A;    IF  SUBSTR(V_PROD_DIMS,2,1) = &apos;L&apos; THEN  &#x0D;&#x0A;            &#x0D;&#x0A;--        V_PROD_DIMS :=  &apos;D&apos; || SUBSTR(CUR1_R.PROD_DIMS,2,LENGTH(CUR1_R.PROD_DIMS)-1);&#x0D;&#x0A;      V_PROD_DIMS := REPLACE(CUR1_R.PROD_DIMS,&apos;L&apos;,&apos;D&apos;);   --BL -&gt; BD 로 변환&#x0D;&#x0A;        DBMS_OUTPUT.PUT_LINE(&apos;V_PROD_DIMS B:&apos; || V_PROD_DIMS);&#x0D;&#x0A;    END IF;&#x0D;&#x0A;  &#x0D;&#x0A;  &#x0D;&#x0A;    IF CUR1_R.DRIVER_TEL_NO IS NULL THEN&#x0D;&#x0A;      DBMS_OUTPUT.PUT_LINE(&apos;DRIVER_TEL_NO AND MTRL_CHG_CD IS NLL&apos;);&#x0D;&#x0A;      &#x0D;&#x0A;      UPDATE TB_MES_IF_MTRL_STOCK SET&#x0D;&#x0A;          ERR_MSG = &apos;MTRL_CHG_CD IS NULL&apos;&#x0D;&#x0A;        WHERE  WORKS_CD       = CUR1_R.WORKS_CD&#x0D;&#x0A;           -- AND  FAC_CD         = CUR1_R.FAC_CD&#x0D;&#x0A;            AND  IF_XI_GROUP_ID = CUR1_R.IF_XI_GROUP_ID;  &#x0D;&#x0A;      CONTINUE;&#x0D;&#x0A;    END IF;&#x0D;&#x0A;  &#x0D;&#x0A;    V_MTRL_CHG_CD := CUR1_R.DRIVER_TEL_NO;&#x0D;&#x0A;    DBMS_OUTPUT.PUT_LINE(&apos;V_MTRL_CHG_CD: &apos; || V_MTRL_CHG_CD);&#x0D;&#x0A;    DBMS_OUTPUT.PUT_LINE(&apos;CUR1_R.DRIVER_TEL_NO: &apos; || CUR1_R.DRIVER_TEL_NO);&#x0D;&#x0A;  &#x0D;&#x0A;    BEGIN &#x0D;&#x0A;       SELECT CD_V_MEANING  INTO V_MTRL_CHG_CAUSE&#x0D;&#x0A;         FROM TB_MES_CODE_DTL&#x0D;&#x0A;        WHERE WORKS_CD = V_WORKS_CD&#x0D;&#x0A;          AND FAC_CD   = V_FAC_CD&#x0D;&#x0A;          AND CD_TP = &apos;MTRL_CHG_CD&apos;&#x0D;&#x0A;          AND CD_V = DECODE(V_MTRL_CHG_CD, &apos;H&apos;, &apos;3&apos;, &apos;S&apos;, &apos;5&apos;);&#x0D;&#x0A;    EXCEPTION &#x0D;&#x0A;      WHEN OTHERS THEN&#x0D;&#x0A;         DBMS_OUTPUT.PUT_LINE(&apos;재고변동사유 CODE가 공통코드 TABLE에 미등록 !&apos;);&#x0D;&#x0A;          &#x0D;&#x0A;         UPDATE TB_MES_IF_MTRL_STOCK SET  &#x0D;&#x0A;                  ERR_MSG   =   &apos;재고변동사유 CODE가 공통코드 TABLE에 미등록 !&apos; &#x0D;&#x0A;           WHERE  WORKS_CD       = CUR1_R.WORKS_CD&#x0D;&#x0A;--             AND  FAC_CD         = CUR1_R.FAC_CD&#x0D;&#x0A;             AND  IF_XI_GROUP_ID = CUR1_R.IF_XI_GROUP_ID; &#x0D;&#x0A;             &#x0D;&#x0A;         CONTINUE;      &#x0D;&#x0A;    END;&#x0D;&#x0A;  &#x0D;&#x0A;    V_DIMS_CD := NULL;&#x0D;&#x0A;  &#x0D;&#x0A;    BEGIN &#x0D;&#x0A;        SELECT CD_V INTO V_DIMS_CD&#x0D;&#x0A;      FROM   TB_MES_CODE_DTL&#x0D;&#x0A;      WHERE WORKS_CD = V_WORKS_CD  AND  FAC_CD  = V_FAC_CD&#x0D;&#x0A;      AND   CD_TP    = &apos;DIMS_CD&apos;&#x0D;&#x0A;      AND   REF_CD_1 = V_PROD_DIMS      --치수코드&#x0D;&#x0A;      AND   REF_CD_2 = CUR1_R.SPEC_CD;  --규격약호&#x0D;&#x0A;    EXCEPTION &#x0D;&#x0A;      WHEN OTHERS THEN&#x0D;&#x0A;           DBMS_OUTPUT.PUT_LINE(&apos;치수코드 CODE가 공통코드 TABLE에 미등록 !&apos;);&#x0D;&#x0A;          &#x0D;&#x0A;         UPDATE TB_MES_IF_MTRL_STOCK SET  &#x0D;&#x0A;                  ERR_MSG   =   &apos;치수코드 CODE가 공통코드 TABLE에 미등록, REF1=&apos; || &#x0D;&#x0A;                                 CUR1_R.PROD_DIMS || &apos;,REF2 =&apos; || CUR1_R.SPEC_CD &#x0D;&#x0A;           WHERE  WORKS_CD       = CUR1_R.WORKS_CD&#x0D;&#x0A;--             AND  FAC_CD         = CUR1_R.FAC_CD&#x0D;&#x0A;             AND  IF_XI_GROUP_ID = CUR1_R.IF_XI_GROUP_ID; &#x0D;&#x0A;             &#x0D;&#x0A;         CONTINUE;      &#x0D;&#x0A;    END;&#x0D;&#x0A;    &#x0D;&#x0A;    DBMS_OUTPUT.PUT_LINE(&apos;V_DIMS_CD:&apos; || V_DIMS_CD);&#x0D;&#x0A;      &#x0D;&#x0A;      BEGIN &#x0D;&#x0A;        INSERT  INTO TB_MES_MTRL_RSLT(&#x0D;&#x0A;            TRANSACTION_ID,&#x0D;&#x0A;          WORKS_CD,&#x0D;&#x0A;          FAC_CD,&#x0D;&#x0A;          BASE_DD,&#x0D;&#x0A;          MTRL_CHG_CAUSE_CD,&#x0D;&#x0A;          SUP_CUST_CD,&#x0D;&#x0A;          CUST_CD,&#x0D;&#x0A;          PRODUCT_NAME_CD,&#x0D;&#x0A;          DIMS_CD,&#x0D;&#x0A;          MTRL_WGT,&#x0D;&#x0A;          MTRL_QUANTITY,&#x0D;&#x0A;          WGHT_NO,&#x0D;&#x0A;          VEHL_NO,&#x0D;&#x0A;          MTRL_CHG_CAUSE,&#x0D;&#x0A;          SUP_MTRL_WGT,&#x0D;&#x0A;          CREATED_OBJECT_ID,&#x0D;&#x0A;          CREATED_PROGRAM_ID,&#x0D;&#x0A;          CREATION_TIMESTAMP,&#x0D;&#x0A;          LAST_UPDATED_OBJECT_ID,&#x0D;&#x0A;          LAST_UPDATE_PROGRAM_ID,&#x0D;&#x0A;          LAST_UPDATE_TIMESTAMP&#x0D;&#x0A;      &#x0D;&#x0A;        ) VALUES (&#x0D;&#x0A;            SQ_MES_MTRL_RSLT.NEXTVAL,   --TRANSACTION_ID,&#x0D;&#x0A;          V_WORKS_CD,         --WORKS_CD,&#x0D;&#x0A;          V_FAC_CD,           --FAC_CD,&#x0D;&#x0A;          NVL(CUR1_R.IF_DATE, TO_CHAR(SYSDATE,&apos;YYYYMMDD&apos;)),  --BASE_DD,&#x0D;&#x0A;          DECODE(V_MTRL_CHG_CD, &apos;H&apos;, &apos;3&apos;, &apos;S&apos;, &apos;5&apos;),  --MTRL_CHG_CAUSE_CD :3(입고예정), 85(유통)--, H:이체(임가공)입고로 처리X S:유통(판매)&#x0D;&#x0A;          NVL(V_SUP_CUST_CD, &apos;4001&apos;),          --SUP_CUST_CD,&#x0D;&#x0A;          DECODE(V_MTRL_CHG_CD, &apos;H&apos;, &apos;3333&apos;, &apos;S&apos;, &apos;4444&apos;), --입고예저의 경우 CUST_CD: 3333(제강사현장명), 유통의 경우 CUST_CD: 4444(금문철강 유통)&#x0D;&#x0A;          SUBSTR(V_PROD_DIMS,1,2),  --PRODUCT_NAME_CD,&#x0D;&#x0A;          V_DIMS_CD,              --DIMS_CD,&#x0D;&#x0A;          NVL(CUR1_R.PROD_WGT,0),   --MTRL_WGT,&#x0D;&#x0A;          NVL(CUR1_R.PROD_PIECE_NO,0),      --PIECE_NO,&#x0D;&#x0A;          CUR1_R.WGHT_NO,       --WGHT_NO,&#x0D;&#x0A;          CUR1_R.VEHL_NO,       --VEHL_NO,&#x0D;&#x0A;          V_MTRL_CHG_CAUSE,           --MTRL_CHG_CAUSE,&#x0D;&#x0A;          CUR1_R.REAL_WGT,        --REAL_WGT(supp),&#x0D;&#x0A;          &apos;SP&apos;,           --CREATED_OBJECT_ID,&#x0D;&#x0A;          &apos;SP_IF_MTRL_STOCK_RECV_J&apos;,  --CREATED_PROGRAM_ID,&#x0D;&#x0A;          SYSDATE,          --CREATION_TIMESTAMP,&#x0D;&#x0A;          &apos;SP&apos;,           --LAST_UPDATED_OBJECT_ID,&#x0D;&#x0A;          &apos;SP_IF_MTRL_STOCK_RECV_J&apos;,  --LAST_UPDATE_PROGRAM_ID,&#x0D;&#x0A;          SYSDATE           --LAST_UPDATE_TIMESTAMP   &#x0D;&#x0A;        );&#x0D;&#x0A;      &#x0D;&#x0A;      EXCEPTION &#x0D;&#x0A;      WHEN OTHERS THEN&#x0D;&#x0A;           ROLLBACK;&#x0D;&#x0A;         DBMS_OUTPUT.PUT_LINE(&apos;TB_MES_MTRL_RSLT INSERT ERROR, SQLCODE :&apos; || SQLCODE || &apos;SQLERRM :&apos; || SQLERRM);&#x0D;&#x0A;         &#x0D;&#x0A;         UPDATE TB_MES_IF_MTRL_STOCK SET  &#x0D;&#x0A;                  ERR_MSG   =   &apos;TB_MES_MTRL_RSLT INSERT ERR,CODE:&apos; || SQLCODE || &apos;,,ERRM :&apos; || SQLERRM&#x0D;&#x0A;           WHERE  WORKS_CD       = CUR1_R.WORKS_CD&#x0D;&#x0A;--             AND  FAC_CD         = CUR1_R.FAC_CD&#x0D;&#x0A;             AND  IF_XI_GROUP_ID = CUR1_R.IF_XI_GROUP_ID; &#x0D;&#x0A;             &#x0D;&#x0A;         CONTINUE;  &#x0D;&#x0A;      END;&#x0D;&#x0A;      &#x0D;&#x0A;      -- 진천공장이 아닌경우와 유통(판매)아닌 경우 재고TABLE 반영 안함 ---&#x0D;&#x0A;      IF  CUR1_R.WORKS_CD &lt;&gt; &apos;J&apos;  THEN&#x0D;&#x0A;         DBMS_OUTPUT.PUT_LINE(&apos;WORKS_CD_FROM :&apos; || CUR1_R.WORKS_CD); &#x0D;&#x0A;         &#x0D;&#x0A;         CONTINUE;&#x0D;&#x0A;      END IF;&#x0D;&#x0A;      &#x0D;&#x0A;      DBMS_OUTPUT.PUT_LINE(&apos;TB_MES_MTRL_RSLT INSERT OK : &apos;);&#x0D;&#x0A;      &#x0D;&#x0A;      &#x0D;&#x0A;      IF  V_MTRL_CHG_CD &lt;&gt; &apos;S&apos;  THEN &#x0D;&#x0A;         DBMS_OUTPUT.PUT_LINE(&apos;V_MTRL_CHG_CD FROM :&apos; || V_MTRL_CHG_CD || &apos;, &apos; || V_MTRL_CHG_CAUSE); &#x0D;&#x0A;         DBMS_OUTPUT.PUT_LINE(&apos;입고 예정 상태의 경우는 재고에 추가되지 않습니다.&apos; ); &#x0D;&#x0A;      ELSE&#x0D;&#x0A;      &#x0D;&#x0A;      V_MTRL_STOCK_CNT :=  0; &#x0D;&#x0A;      &#x0D;&#x0A;      V_CUST_CD := NULL;&#x0D;&#x0A;      V_CUST_CD :=  &apos;4444&apos;; --금문철강 유통판매(임시) 코드 &#x0D;&#x0A;    &#x0D;&#x0A;      SELECT COUNT(*) INTO  V_MTRL_STOCK_CNT&#x0D;&#x0A;          FROM TB_MES_MTRL_STOCK&#x0D;&#x0A;         WHERE WORKS_CD     = V_WORKS_CD &#x0D;&#x0A;           AND FAC_CD       = V_FAC_CD &#x0D;&#x0A;           AND SUP_CUST_CD    = V_SUP_CUST_CD &#x0D;&#x0A;           AND PRODUCT_NAME_CD  = SUBSTR(V_PROD_DIMS,1,2)&#x0D;&#x0A;           AND CUST_CD      = V_CUST_CD&#x0D;&#x0A;           AND DIMS_CD      = V_DIMS_CD;&#x0D;&#x0A;           &#x0D;&#x0A;        DBMS_OUTPUT.PUT_LINE(&apos;V_MTRL_STOCK_CNT :&apos; || V_MTRL_STOCK_CNT); &#x0D;&#x0A;      &#x0D;&#x0A;      &#x0D;&#x0A;      IF  NVL(V_MTRL_STOCK_CNT,0) &gt; 0   THEN&#x0D;&#x0A;          DBMS_OUTPUT.PUT_LINE(&apos;MTRL_STOCK_matched :&apos;); &#x0D;&#x0A;          UPDATE TB_MES_MTRL_STOCK   SET&#x0D;&#x0A;                 MTRL_STOCK_WGT = MTRL_STOCK_WGT + NVL(CUR1_R.PROD_WGT,0),&#x0D;&#x0A;                 LAST_UPDATED_OBJECT_ID = &apos;SP&apos;,&#x0D;&#x0A;           LAST_UPDATE_PROGRAM_ID = &apos;SP_IF_MTRL_STOCK_RECV_J&apos;,&#x0D;&#x0A;           LAST_UPDATE_TIMESTAMP  = SYSDATE&#x0D;&#x0A;        WHERE WORKS_CD      = V_WORKS_CD&#x0D;&#x0A;            AND FAC_CD        = V_FAC_CD&#x0D;&#x0A;            AND SUP_CUST_CD     = V_SUP_CUST_CD &#x0D;&#x0A;            AND PRODUCT_NAME_CD   = SUBSTR(V_PROD_DIMS,1,2)&#x0D;&#x0A;            AND CUST_CD     = V_CUST_CD&#x0D;&#x0A;            AND DIMS_CD       = V_DIMS_CD;&#x0D;&#x0A;      &#x0D;&#x0A;      ELSE&#x0D;&#x0A;          DBMS_OUTPUT.PUT_LINE(&apos;MTRL_STOCK_not matched :&apos;); &#x0D;&#x0A;        BEGIN&#x0D;&#x0A;            INSERT INTO TB_MES_MTRL_STOCK (&#x0D;&#x0A;              WORKS_CD,&#x0D;&#x0A;              FAC_CD,&#x0D;&#x0A;              SUP_CUST_CD,&#x0D;&#x0A;              CUST_CD,&#x0D;&#x0A;              PRODUCT_NAME_CD,&#x0D;&#x0A;              DIMS_CD,&#x0D;&#x0A;              MTRL_STOCK_WGT,&#x0D;&#x0A;              CREATED_OBJECT_ID,&#x0D;&#x0A;                CREATED_PROGRAM_ID,&#x0D;&#x0A;              CREATION_TIMESTAMP,&#x0D;&#x0A;              LAST_UPDATED_OBJECT_ID,&#x0D;&#x0A;              LAST_UPDATE_PROGRAM_ID,&#x0D;&#x0A;              LAST_UPDATE_TIMESTAMP       &#x0D;&#x0A;            ) &#x0D;&#x0A;            VALUES (&#x0D;&#x0A;              V_WORKS_CD,&#x0D;&#x0A;              V_FAC_CD,&#x0D;&#x0A;              V_SUP_CUST_CD,&#x0D;&#x0A;              V_CUST_CD,&#x0D;&#x0A;              SUBSTR(V_PROD_DIMS,1,2),&#x0D;&#x0A;              V_DIMS_CD,&#x0D;&#x0A;              NVL(CUR1_R.PROD_WGT,0),&#x0D;&#x0A;              &apos;SP&apos; ,&#x0D;&#x0A;                &apos;SP_IF_MTRL_STOCK_RECV_J&apos;,&#x0D;&#x0A;              SYSDATE,&#x0D;&#x0A;              &apos;SP&apos;,&#x0D;&#x0A;              &apos;SP_IF_MTRL_STOCK_RECV_J&apos;,&#x0D;&#x0A;              SYSDATE       &#x0D;&#x0A;           );&#x0D;&#x0A;        EXCEPTION &#x0D;&#x0A;        WHEN OTHERS THEN&#x0D;&#x0A;          ROLLBACK;&#x0D;&#x0A;        DBMS_OUTPUT.PUT_LINE(&apos;TB_MES_MTRL_STOCK INSERT ERROR, SQLCODE :&apos; || SQLCODE || &apos;SQLERRM :&apos; || SQLERRM);&#x0D;&#x0A;        &#x0D;&#x0A;        UPDATE  TB_MES_IF_MTRL_STOCK SET  &#x0D;&#x0A;                      ERR_MSG   =   &apos;TB_MES_MTRL_STOCK INSERT ERROR,CODE :&apos; || SQLCODE || &apos;,ERRM :&apos; || SQLERRM&#x0D;&#x0A;             WHERE  WORKS_CD       = CUR1_R.WORKS_CD&#x0D;&#x0A;--               AND  FAC_CD         = CUR1_R.FAC_CD&#x0D;&#x0A;               AND  IF_XI_GROUP_ID = CUR1_R.IF_XI_GROUP_ID; &#x0D;&#x0A;             &#x0D;&#x0A;        CONTINUE;    &#x0D;&#x0A;        END;&#x0D;&#x0A;          &#x0D;&#x0A;      END IF;&#x0D;&#x0A;     &#x0D;&#x0A;     END IF; &#x0D;&#x0A;       &#x0D;&#x0A;        DBMS_OUTPUT.PUT_LINE(&apos;TB_MES_MTRL_STOCK INSERT/UPDATE OK : &apos;);&#x0D;&#x0A;        &#x0D;&#x0A;        UPDATE  TB_MES_IF_MTRL_STOCK SET &#x0D;&#x0A;                RECV_FLAG      =   &apos;Y&apos;,&#x0D;&#x0A;                RECV_DATE      =   TO_CHAR(SYSDATE, &apos;YYYYMMDD&apos;),&#x0D;&#x0A;                RECV_TIME      =   TO_CHAR(SYSDATE, &apos;HH24MISS&apos;),&#x0D;&#x0A;                ERR_MSG        =   NULL&#x0D;&#x0A;         WHERE  WORKS_CD       = CUR1_R.WORKS_CD&#x0D;&#x0A;--           AND  FAC_CD         = CUR1_R.FAC_CD&#x0D;&#x0A;           AND  IF_XI_GROUP_ID = CUR1_R.IF_XI_GROUP_ID;    &#x0D;&#x0A;        &#x0D;&#x0A;        DBMS_OUTPUT.PUT_LINE(&apos;TB_MES_IF_MTRL_STOCK UPDATE OK : &apos;);&#x0D;&#x0A;        COMMIT;   &#x0D;&#x0A;            &#x0D;&#x0A;  END LOOP;&#x0D;&#x0A;&#x0D;&#x0A;    DBMS_OUTPUT.PUT_LINE(&apos;LOOP OUT !!&apos;);&#x0D;&#x0A;    &#x0D;&#x0A;   -- 커서의 CLOSE&#x0D;&#x0A;    CLOSE CUR_1;    &#x0D;&#x0A;     &#x0D;&#x0A;    COMMIT; &#x0D;&#x0A;    DBMS_OUTPUT.PUT_LINE(&apos;SP_IF_MTRL_STOCK_RECV_J, SUCCESS !!&apos;);&#x0D;&#x0A;EXCEPTION &#x0D;&#x0A;    WHEN OTHERS THEN&#x0D;&#x0A;        ROLLBACK;&#x0D;&#x0A;        DBMS_OUTPUT.PUT_LINE(&apos;SP_IF_MTRL_STOCK_RECV_J, FAILED !!&apos;);&#x0D;&#x0A;END;&#x0D;&#x0A;</text>
